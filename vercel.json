{
    "version": 2,
    "buildCommand": "bash build_vercel.sh",
    "outputDirectory": "staticfiles",
    "rewrites": [
        {
            "source": "/static/(.*)",
            "destination": "/static/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/edshop/wsgi.py"
        }
    ]
}
